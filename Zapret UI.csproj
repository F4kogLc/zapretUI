    <None Remove="zapret\task_start.cmd" />
    <None Remove="zapret\task_stop.cmd" />
    <None Remove="zapret\tools\elevator.vbs" />
    <None Remove="zapret\WinDivert.dll" />
    <None Remove="zapret\WinDivert64.sys" />
    <None Remove="zapret\windivert_delete.cmd" />
    <None Remove="zapret\winws.exe" />
    <None Remove="zapret\_CMD_ADMIN.cmd" />
