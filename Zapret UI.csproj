    <None Remove="zapret\quic_initial_www_google_com.bin" />
    <None Remove="zapret\tls_clienthello_www_google_com.bin" />
    <None Remove="zapret\zapret-winws\files\list-youtube.txt" />
    <None Remove="zapret\zapret-winws\files\quic_initial_www_google_com.bin" />
    <None Remove="zapret\killall.exe" />
    <None Remove="zapret\preset_russia.cmd" />
